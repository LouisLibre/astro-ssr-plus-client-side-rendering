---
export const prerender = true;
import App from "../../components/DynamicApp";
const url = Astro.request.url;
const pathname = new URL(url).pathname;
export function getStaticPaths() {
  return [{ params: { path: undefined } }, { params: { route: "page1" }}, { params: { route: "page2" } }];
}
---
<html>
<head>
</head>
<body>
<h1>Astro title</h1>
  <App client:only="react" url={url} pathname={pathname} />
</body>
</html>
